{
  "name": "agent-rogue",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "predev": "node scripts/check-node.mjs",
    "dev": "vite",
    "prebuild": "node scripts/check-node.mjs",
    "build": "tsc -b && vite build",
    "lint": "eslint \"src/**/*.{js,jsx}\" --no-error-on-unmatched-pattern",
    "lint:fix": "eslint \"src/**/*.{js,jsx}\" --fix --no-error-on-unmatched-pattern",
    "typecheck": "tsc -p tsconfig.json --noEmit || exit 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.6",
    "typescript": "^5.5.3",
    "vite": "^5.3.4"
  },
  "engines": {
    "node": ">=20"
  }
}
